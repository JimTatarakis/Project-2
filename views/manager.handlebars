    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h2>Manager View</h2>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="example-text">Assgin Task To:</label>
            <input type="text" id="example-text" class="form-control" aria-describedby="example-text"
              placeholder="Employee Name">
          </div>
          <div class="form-group">
            <label for="example-description">Description of Task:</label>
            <textarea class="form-control" id="example-description" aria-describedby="example-description"
              placeholder="Description"></textarea>
          </div>
          <button id="submit" class="btn btn-primary float-right">Add Task</button>
        </form>
      </div>
      <div class="row">
        <div class="col-4 ">
          <div class="card" style="width: 18rem;">
            <img src="..." class="card-img-top" alt="...">
            <div class="card-body">
              <h5 class="card-title">Today's Tasks</h5>
              <p class="card-text">Tasks to be completed by end of day.</p>
            </div>
            <ul id="example-list" class="list-group list-group-flush list-group">
              {{#each examples}}
              <li class="list-group-item" data-id="{{this.id}}">
                <a href="example/{{this.id}}">{{this.text}}</a>
                <button class="btn btn-danger float-right delete">ｘ</button>
              </li>
               {{/each}}
            </ul>
        </div>
        
        </div>
